<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <PackageId>Tcp.NET</PackageId>
    <Company>Rob Engel - LiveOrDevTrying - Pixel Horror Studios</Company>
    <Description>The server and client modules for a simple SSL Tcp Server with Authorization and Client implementation.</Description>
    <Copyright>Copyright 2019</Copyright>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/LiveOrDevTrying/Tcp.NET</PackageProjectUrl>
    <PackageIconUrl></PackageIconUrl>
    <RepositoryUrl>https://github.com/LiveOrDevTrying/Tcp.NET</RepositoryUrl>
    <RepositoryType>Github Public</RepositoryType>
    <PackageTags>Tcp server client Tcp.NET TcpNET async TcpSharp dotnet net 2.1 2.0 SSL authorization auth</PackageTags>
    <PackageReleaseNotes></PackageReleaseNotes>
    <Version>1.1.10</Version>
    <Authors>Rob Engel - LiveOrDevTrying</Authors>
    <PackageIcon>Logo.png</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Include="Logo.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Tcp.NET.Client" Version="1.1.6" />
    <PackageReference Include="Tcp.NET.Client.SSL" Version="1.0.4" />
    <PackageReference Include="Tcp.NET.Server.Auth" Version="1.1.6" />
    <PackageReference Include="Tcp.NET.Server.SSL.Auth" Version="1.0.4" />
  </ItemGroup>

</Project>
